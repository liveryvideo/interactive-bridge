{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./dist/index.d.ts"],
  "tsconfig": "./tsconfig-typedoc.jsonc",
  "out": "dist/doc",
  "plugin": ["typedoc-plugin-no-inherit", "typedoc-plugin-markdown"],
  "categorizeByGroup": true,
  "disableSources": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeNotDocumented": true,
  "includeVersion": true,
  "readme": "none",
  // TODO: Once we update typedoc: "suppressCommentWarningsInDeclarationFiles": true,
  /* typedoc-plugin-markdown */
  "entryFileName": "index",
  "hidePageHeader": true
}
