{
  "$schema": "https://typedoc.org/schema.json",
  "categorizeByGroup": true,
  "disableSources": true,
  "entryFileName": "index", // typedoc-plugin-markdown
  "entryPoints": [
    "./dist/index.d.ts"
  ],
  "excludeNotDocumented": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "hidePageHeader": true, // typedoc-plugin-markdown
  "includeVersion": true,
  "out": "dist/doc",
  "plugin": [
    "typedoc-plugin-no-inherit",
    "typedoc-plugin-markdown"
  ],
  "readme": "none",
  // TODO: Once we update typedoc: "suppressCommentWarningsInDeclarationFiles": true,
  "tsconfig": "./tsconfig-typedoc.jsonc"
}
