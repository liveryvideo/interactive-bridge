{
  "name": "@liveryvideo/interactive-bridge",
  "version": "1.6.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Livery Video Interactive Bridge.",
  "author": "Livery Video",
  "homepage": "https://www.liveryvideo.com/",
  "license": "MIT",
  "repository": "github:liveryvideo/interactive-bridge",
  "main": "dist/index.umd.js",
  "module": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.umd.js"
  },
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dist:demo": "rollup -m -c rollup.config.demo.js",
    "dist:watch": "rollup -c -w",
    "dist": "rollup -m -c",
    "format:eslint": "eslint --ext .ts,.js,.html . --fix",
    "format:markdown": "markdownlint \"**/*.md\" --fix",
    "format:prettier": "prettier \"**/*.{html,js,json,md,ts}\" --write",
    "format": "npm run format:eslint && npm run format:markdown && npm run format:prettier",
    "lint:eslint": "eslint --ext .ts,.js,.html .",
    "lint:markdown": "markdownlint \"**/*.md\"",
    "lint:prettier": "prettier \"**/*.{html,js,json,md,ts}\" --check",
    "lint:typescript": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:markdown && npm run lint:prettier && npm run lint:typescript",
    "prepare": "husky install",
    "prepublishOnly": "npm run dist",
    "start": "web-dev-server --open 'demo/'",
    "test:watch": "web-test-runner --watch",
    "test": "web-test-runner"
  },
  "dependencies": {
    "lit": "^2.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@open-wc/eslint-config": "^8.0.2",
    "@open-wc/testing": "^3.1.6",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@web/dev-server": "^0.1.35",
    "@web/dev-server-esbuild": "^0.3.3",
    "@web/rollup-plugin-html": "^1.11.0",
    "@web/test-runner": "^0.15.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "git-branch-is": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.2",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.5",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{html,js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "markdownlint --fix",
      "prettier --write"
    ]
  }
}
